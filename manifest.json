{
    "name": "RandPass",
    "version": "0.1.0",
    "description": "Generates a random password",
    "action": {
      "default_title": "RandPass",
      "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
      "storage"
    ],
    "icons": {
      "16": "assets/icons/icon-16.png",
      "48": "assets/icons/icon-48.png",
      "128": "assets/icons/icon-128.png"
    },
    "manifest_version": 3
}